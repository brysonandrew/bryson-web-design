{
  "name": "@brysonandrew/bryson-web-design",
  "version": "7.0.1",
  "description": "Building websites and apps",
  "main": "src/index.tsx",
  "types": "src/index.d.ts",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "vite build --config vite.config.ts",
    "format": "prettier .",
    "lint": "tsc --build . & eslint . --fix --ext ts,tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "screens": "esrun ops/screens/process",
    "bump": "npm version major --workspaces",
    "publish": "npm run bump && npm publish --workspaces",
    "service-worker": "npx nodemon --exec 'yarn build && yarn dev' -w src -w vite.config.ts -e 'ts,js,tsx,jsx'",
    "clean": "esrun ops/delete/clean.ts",
    "exporter": "esrun ops/exporter/main.ts test"
  },
  "workspaces": [
    "test",
    "test/sounds"
  ],
