{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ops/*": ["ops/*"],
      "@pkg": ["package.json"],
      "@brysonandrew/*": ["lib/*", "ops/*"],
      "@brysonandrew/gallery-*": ["lib/gallery/*"],
      "@brysonandrew/uno-*": ["lib/uno/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@context/*": ["src/context/*"],
      "@css/*": ["src/css/*"],
      "@hooks/*": ["src/hooks/*"],
      "@icons/*": ["src/icons/*"],
      "@logic/*": ["src/logic/*"],
      "@main/*": ["src/main/*"],
      "@pages/*": ["src/pages/*"],
      "@t/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@app/*": ["config/app/*"],
      "@uno/*": ["config/uno/*"],
      "@shell/*": ["src/shell/*"]
    }
  }
}
