  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.ts",
    "lint": "tsc --build . & eslint . --fix --ext ts,tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "screens": "esrun ops/screens/process",
    "d": "npm run dev",
    "e": "esrun ops/exporter lib",
    "p": "npm publish --ws",
    "l": "npm run lint",
    "b": "npm run build",
    "f": "prettier .",
    "vw": "npm version minor --ws",
    "uw": "npm upgrade --ws",
    "iw": "npm install --ws",
    "el": "npm run e && npm run l",
    "epl": "npm run e && npm run p && npm run l",
    "service-worker": "npx nodemon --exec 'yarn build && yarn dev' -w src -w vite.config.ts -e 'ts,js,tsx,jsx'",
    "clean": "esrun ops/delete/clean.ts",
    "fonts": "esrun ops/fonts/index.ts",
    "ets": "esrun ops/resolve-ts-paths.ts",
