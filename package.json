{
  "name": "@brysonandrew/bryson-web-design",
  "version": "7.0.1",
  "description": "Building websites and apps",
  "main": "src/index.tsx",
  "types": "src/index.d.ts",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "build": "vite build --config vite.config.ts",
    "format": "prettier .",
    "lint": "tsc --build . & eslint . --fix --ext ts,tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "screens": "esrun ops/screens/process",
    "bump": "npm version minor --workspaces",
    "publish": "npm run bump && npm publish --workspaces",
    "service-worker": "npx nodemon --exec 'yarn build && yarn dev' -w src -w vite.config.ts -e 'ts,js,tsx,jsx'",
    "clean": "esrun ops/delete/clean.ts",
    "exporter": "esrun ops/exporter/main.ts lib && npm run lint"
  },
  "workspaces": [
    "lib/animation",
    "lib/app",
    "lib/base",
    "lib/config",
    "lib/contact",
    "lib/cursor",
    "lib/dark-mode",
    "lib/filter-animate",
    "lib/filters",
    "lib/gallery",
    "lib/head",
    "lib/hooks",
    "lib/in-view",
    "lib/media",
    "lib/parallax",
    "lib/scroll",
    "lib/sounds",
    "lib/viewport"
  ],
  "author": "Andrew Bryson <andrewbryson12@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/brysonandrew/brysona.dev.git",
  "bugs": {
    "url": "https://github.com/brysonandrew/brysona.dev/issues"
  },
  "packageManager": "yarn@3.4.1",
  "dependencies": {
    "@emailjs/browser": "^3.12.1",
    "clsx": "^1.2.1",
    "framer-motion": "^10.18.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^9.5.3",
    "react-router": "^6.21.1",
    "react-router-dom": "^6.21.3",
    "react-synthwave": "^1.2.7",
    "webrtc-adapter": "^8.2.3"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.26",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@iconify/json": "^2.2.171",
    "@iconify/react": "^4.1.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.19.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@types/recordrtc": "^5.6.14",
    "@types/webrtc": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@unocss/transformer-variant-group": "^0.55.7",
    "@vite/vitejs-client": "^2.2.10",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.56.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "prettier": "^2.8.8",
    "rollup": "^3.29.4",
    "rollup-plugin-ts": "^3.4.5",
    "rollup-plugin-typescript": "^1.0.1",
    "sharp": "^0.32.6",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "unocss": "^0.53.6",
    "vite": "^4.5.2",
    "vite-tsconfig-paths": "^3.6.0"
  },
  "publishConfig": {
    "@brysonandrew:registry": "https://npm.pkg.github.com"
  },
  "homepage": "brysona-web-design.co.nz"
}