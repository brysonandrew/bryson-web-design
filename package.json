{
  "name": "brysona.dev",
  "version": "1.0.0",
  "description": "Building websites and apps",
  "main": "src/main/index.tsx",
  "config": {
    "blur_images_dir_path": "ops/blur-images",
    "vite_config_path": "vite.config.ts"
  },
  "scripts": {
    "dev": "vite --config $npm_package_config_vite_config_path",
    "sw": "npx nodemon --exec 'yarn build && yarn dev' -w src -w $npm_package_config_vite_config_path -e 'ts,js,tsx,jsx'",
    "build": "vite build --config $npm_package_config_vite_config_path",
    "format": "prettier .",
    "lint": "tsc --build . & eslint . --fix --ext ts,tsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "screens-process": "esrun ops/process",
    "blur-images/backup": "esrun $blur_images_dir_path/backup",
    "blur-images/clean": "esrun $blur_images_dir_path/clean",
    "blur-images/reset": "esrun $blur_images_dir_path/reset",
    "blur-images": "esrun $blur_images_dir_path/backup && $blur_images_dir_path/blur.sh && $blur_images_dir_path/blur.sh webp && esrun $blur_images_dir_path/clean",
    "d": "esrun ops/delete/deleteDoubleProcessed"
  },
  "author": "Andrew Bryson <andrewbryson12@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brysonandrew/brysona.dev.git"
  },
  "bugs": {
    "url": "https://github.com/brysonandrew/brysona.dev/issues"
  },
  "packageManager": "yarn@1.22.19",
  "types": "src/index.d.ts",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^3.10.0",
    "ably": "^1.2.44",
    "clsx": "^1.2.1",
    "framer-motion": "^10.3.2",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^9.5.2",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-synthwave": "^1.2.7",
    "webrtc-adapter": "^8.2.3"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.24",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@iconify/json": "^2.2.119",
    "@iconify/react": "^4.1.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.6",
    "@types/recordrtc": "^5.6.11",
    "@types/webrtc": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@unocss/transformer-variant-group": "^0.55.2",
    "@vite/vitejs-client": "^2.2.10",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.21.0",
    "fast-glob": "^3.3.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "rollup": "^3.28.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sharp": "^0.32.5",
    "tslib": "^2.6.1",
    "typescript": "^4.9.4",
    "unocss": "^0.53.4",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "homepage": "brysona.dev"
}
